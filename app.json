{
    "expo": {
        "name": "Pok√® Budgy",
        "slug": "poke-budgy",
        "version": "0.1.1",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "pokebudgy",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.shakeelansari63.pokebudgy"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/icon.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.shakeelansari63.pokebudgy"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/icon-circle.png"
        },
        "extra": {
            "eas": {
                "projectId": "3236ce7b-34a3-42a4-b69f-9bb2863264aa"
            }
        },
        "owner": "shakeelansari63",
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/icon-splash.png",
                    "imageWidth": 250,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff",
                    "dark": {
                        "backgroundColor": "#262626",
                        "image": "./assets/images/icon-splash-dark.png"
                    }
                }
            ],
            [
                "expo-sqlite",
                {
                    "enableFTS": true,
                    "useSQLCipher": false,
                    "android": {},
                    "ios": {
                        "customBuildFlags": [
                            "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
                        ]
                    }
                }
            ],
            "expo-web-browser",
            "expo-font"
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
